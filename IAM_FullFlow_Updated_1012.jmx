        <BeanShellSampler guiclass="BeanShellSamplerGui" testclass="BeanShellSampler" testname="Script 2: Reading the validation messages from Excel" enabled="true">
          <stringProp name="BeanShellSampler.query">import org.apache.commons.io.FileUtils;    //necessary import
import org.apache.jmeter.gui.GuiPackage;
import org.apache.commons.io.FilenameUtils; 


try {

